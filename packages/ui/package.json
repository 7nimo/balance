    "svgo": "rimraf ./src/assets/svg/icons/optimized  & svgo -q -p 8 -f ./src/assets/svg/icons/raw -o ./src/assets/svg/icons/optimized",
    "svgr": "npx @svgr/cli --config-file svgo.yml ./src/common/components/icons ./src/assets/svg/icons/optimized",
    "icon-gen": "npx @svgr/cli --icon -d ./src/components/Icons ./src/assets/svg/icons/optimized"
  },
